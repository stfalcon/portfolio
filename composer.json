{
    "name": "stfalcon.com",
    "description": "",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "behat.org"
        },
        {
            "type": "package",
            "package": {
                "name": "zendframework/zend-cache",
                "version": "2.0.0beta1",
                "dist": {
                    "url": "http://github.com/KnpLabs/zend-cache/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/KnpLabs/zend-cache.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": {
                        "Zend\\Cache": ""
                    }
                },
                "target-dir": "Zend/Cache"
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zendframework/zend-filter",
                "version": "2.0.0beta1",
                "dist": {
                    "url": "http://github.com/KnpLabs/zend-filter/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/KnpLabs/zend-filter.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": {
                        "Zend\\Filter": ""
                    }
                },
                "target-dir": "Zend/Filter"
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zendframework/zf1",
                "version": "1.11",
                "autoload": { "psr-0": { "Zend_": "library" } },
                "dist": {
                    "url": "http://github.com/stfalcon-studio/zf/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/stfalcon-studio/zf.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zendframework/zf2",
                "version": "2.0.0",
                "autoload": { "psr-0": { "Zend": "library" } },
                "dist": {
                    "url": "http://github.com/zendframework/zf2/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/zendframework/zf2.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "2.1.*",
        "doctrine/orm": "2.2.0",
        "doctrine/doctrine-bundle": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "twig/extensions": "dev-master",
        "symfony/assetic-bundle": "dev-master",
        "symfony/swiftmailer-bundle": "dev-master",
        "symfony/monolog-bundle": "dev-master",
        "sensio/distribution-bundle": "dev-master",
        "sensio/framework-extra-bundle": "dev-master",
        "sensio/generator-bundle": "dev-master",
        "jms/security-extra-bundle": "1.1.0",
        "jms/di-extra-bundle": "1.0.1",
        "kriswallsmith/assetic": "dev-master",
        "gedmo/doctrine-extensions": "dev-master",
        "stof/doctrine-extensions-bundle": "dev-master",
        "avalanche123/imagine-bundle": "dev-master",
        "knplabs/knp-menu-bundle": "dev-master",
        "knplabs/knp-paginator-bundle": "dev-master",
        "knplabs/knp-zend-cache-bundle": "*",
        "liip/functional-test-bundle": "dev-master",
        "sonata-project/admin-bundle": "dev-master",
        "sonata-project/jquery-bundle": "dev-master",
        "zendframework/zf1": "1.11",
        "zendframework/zf2": "2.0.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}